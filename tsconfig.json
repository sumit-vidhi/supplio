{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "googlemaps"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@app/*": [
        "src/app/*"
      ],
      "@configs/*": [
        "src/app/configs/*"
      ],
      "@core/*": [
        "src/app/core/*"
      ],
      "@interceptors/*": [
        "src/app/core/interceptors/*"
      ],
      "@theme/*": [
        "src/app/modules/theme/*"
      ],
      "@modules/*": [
        "src/app/modules/*"
      ],
      "@environment/*": [
        "src/environments/*"
      ],
      "@shared/*": [
        "src/app/modules/shared/*"
      ]
    }
  }
}